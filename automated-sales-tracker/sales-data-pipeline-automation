        "import pandas as pd\n",
        "\n",
        "# Load the Excel file, using the 3rd row (index 2) as the header\n",
        "df = pd.read_excel(\"Adidas US Sales Datasets.xlsx\", header=0)\n",
        "\n",
        "# Strip whitespace from column names\n",
        "df.columns = df.columns.str.strip()\n",
        "\n",
        "# Preview the cleaned data\n",
        "print(df.head())"
      ],
